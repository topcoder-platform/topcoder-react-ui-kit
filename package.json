  "repository": {
    "type": "git",
    "url": "git+https://github.com/topcoder-platform/topcoder-react-ui-kit.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:dev && npm run build:prod",
    "build:dev": "webpack --env=development --progress --profile --colors --display-optimization-bailout --bail",
    "build:dev:watch": "npm run clean && webpack --env=development --progress --profile --colors --display-optimization-bailout --bail --watch",
    "build:prod": "./node_modules/.bin/webpack --env=production --progress --profile --colors --display-optimization-bailout",
    "clean": "rimraf dist",
    "jest": "jest --no-cache --maxWorkers=4 --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.scss --syntax scss",
    "test": "npm run lint && npm run jest"
  },
  "version": "2.0.0",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-css-super-themr": "^2.2.0",
    "react-dom": "^16.4.1",
    "topcoder-react-utils": "^0.7.8"
  },
